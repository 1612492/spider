{
  "manifest_version": 3,
  "name": "Spider Wallet",
  "short_name": "Spider",
  "version": "0.0.1",
  "description": "Ethereum wallet",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage"]
}
