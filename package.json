{
  "name": "spider",
  "version": "1.0.0",
  "description": "Decentralized wallet for cross-chain",
  "main": "index.js",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "pnpm build:dev && run-p run:chrome watch",
    "run:chrome": "web-ext run -t chromium -s dist",
    "watch": "watch \"pnpm build:dev\" src static",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --ignore-unknown --write .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@sveltejs/kit": "^1.27.6",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-svelte": "^2.35.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "sveltekit-adapter-chrome-extension": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "watch": "^1.0.2",
    "web-ext": "^7.8.0"
  },
  "dependencies": {
    "clsx": "^2.0.0"
  }
}
